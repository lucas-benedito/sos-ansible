{
    "Hub Info": {
        "files": [
            "settings.py"
        ],
        "path": "etc/pulp",
        "query": ""
    },
    "Filesystem": {
        "files": [
            "df"
        ],
        "path": "",
        "query": "100%"
    },
    "noexec": {
        "files": [
            "mount"
        ],
        "path": "",
        "query": "noexec"
    },
    "Messages Errors": {
        "files": [
            "messages"
        ],
        "path": "var/log",
        "query": "error, Traceback, cryptography, LDAPError"
    },
    "OOM": {
        "files": [
            "dmesg"
        ],
        "path": "sos_commands/kernel/",
        "query": "OOM"
    },
    "Antivirus": {
        "files": [
            "installed-rpms"
        ],
        "path": "",
        "query": "falcon, crowd, mcafee"
    },
    "Installed Packages": {
        "files": [
            "installed-rpms"
        ],
        "path": "",
        "query": "ansible, automation, receptor, hub, keycloak, postgres"
    },
    "Running Processes": {
        "files": [
            "ps"
        ],
        "path": "",
        "query": "pulp, nginx"
    },
    "Nginx": {
        "files": [
            "error.log",
            "access.log",
            "automationhub.access.log",
            "automationhub.error.log"
        ],
        "path": "var/log/nginx",
        "query": "error, 504, denied"
    },
    "PIP Conflict": {
        "files": [
            "pip_list"
        ],
        "path": "sos_commands/python",
        "query": "ansible"
    }
